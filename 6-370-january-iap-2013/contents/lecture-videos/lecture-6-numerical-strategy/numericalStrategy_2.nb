          RowBox[{
           RowBox[{"Length", "@", "generators"}], "-", "50"}], ")"}]}]}], 
       "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"myResources", "[", 
          RowBox[{"[", "round", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"myResources", "[", 
           RowBox[{"[", "round", "]"}], "]"}], "-", 
          RowBox[{"20", "*", 
           RowBox[{"(", 
            RowBox[{"encampmentCount", "+", "1"}], ")"}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"suppliers", "[", 
          RowBox[{"[", 
           RowBox[{"round", "+", "50"}], "]"}], "]"}], "=", "1"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"current", "++"}], ";", 
