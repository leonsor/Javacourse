Cell[BoxData[{
 RowBox[{
  RowBox[{"generatorBonus", "[", 
   RowBox[{"round_", ",", "tryToBuild_"}], "]"}], ":=", " ", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "get", " ", "additional", " ", "resources", " ", "from", " ", 
     "generators"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"myResources", "[", 
      RowBox[{"[", "round", "]"}], "]"}], " ", "=", " ", 
     RowBox[{
      RowBox[{"myResources", "[", 
       RowBox[{"[", "round", "]"}], "]"}], "+", 
      RowBox[{"10", "*", "generatorNumber"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", "tryToBuild", ")"}], "\[IndentingNewLine]", "&&", 
       RowBox[{
        RowBox[{"myResources", "[", 
         RowBox[{"[", "round", "]"}], "]"}], ">", 
        RowBox[{
         RowBox[{"20", "*", 
          RowBox[{"(", 
           RowBox[{"encampmentCount", "+", "1"}], ")"}]}], "+", 
         "numberOfBots"}]}], "\[IndentingNewLine]", "&&", 
       RowBox[{"round", "<", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Length", "@", "generators"}], "-", "50"}], ")"}]}]}], 
      "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"myResources", "[", 
         RowBox[{"[", "round", "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"myResources", "[", 
          RowBox[{"[", "round", "]"}], "]"}], "-", 
         RowBox[{"20", "*", 
          RowBox[{"(", 
           RowBox[{"encampmentCount", "+", "1"}], ")"}]}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"generators", "[", 
         RowBox[{"[", 
          RowBox[{"round", "+", "50"}], "]"}], "]"}], "=", "1"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"current", "++"}], ";", 
       RowBox[{"(*", 
        RowBox[{"advance", " ", "build", " ", "order"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"encampmentCount", "++"}], ";"}]}], "\[IndentingNewLine]", 
     "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"generatorNumber", " ", "=", " ", 
     RowBox[{"generatorNumber", "+", 
      RowBox[{"generators", "[", 
       RowBox[{"[", "round", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"supplierBonus", "[", 
